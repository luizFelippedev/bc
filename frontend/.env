NODE_ENV=development

NEXT_PUBLIC_APP_NAME="Portfolio Professional"
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_API_URL=http://localhost:5000

# Meta e Social
NEXT_PUBLIC_TWITTER_HANDLE=@yourhandle
NEXT_PUBLIC_LINKEDIN_URL=https://linkedin.com/in/yourprofile
NEXT_PUBLIC_GITHUB_URL=https://github.com/yourusername

# PWA
NEXT_PUBLIC_PWA_ENABLED=true
NEXT_PUBLIC_PWA_NAME="Portfolio Professional"
NEXT_PUBLIC_PWA_SHORT_NAME="Portfolio"

# Monitoramento
NEXT_PUBLIC_ENABLE_ANALYTICS_TRACKING=true
NEXT_PUBLIC_ENABLE_ERROR_TRACKING=true
GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
MIXPANEL_TOKEN=your-mixpanel-token

# Features
ENABLE_DARK_MODE=true
ENABLE_REAL_TIME_ANALYTICS=true
ENABLE_NOTIFICATIONS=true
ENABLE_MULTILINGUAL=false
ENABLE_BLOG=false
ENABLE_COMMENTS=false

# Links externos
PORTFOLIO_API_URL=https://api.yourportfolio.com
BLOG_API_URL=https://blog.yourportfolio.com/api
DOCS_URL=https://docs.yourportfolio.com
